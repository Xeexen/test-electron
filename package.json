    "@vueuse/motion": "^2.2.6",
    "apexcharts": "4.3.0",
    "auto-launch": "^5.0.6",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "electron-store": "^7.0.0",
    "electron-updater": "^6.1.7",
